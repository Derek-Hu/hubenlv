{
  "name": "@dlpt/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.0.0",
    "classnames": "2.2.5",
    "markdown-it": "^8.4.0",
    "normalize.css": "^7.0.0",
    "prop-types": "15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "styled-components": "^2.2.4"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "deploy": "npm run-all build ftp",
    "analyze": "npm-run-all build analyze-boudle",
    "build": "npm-run-all build-css build-js",
    "mock": "nodemon mock/index.js",
    "test": "react-app-rewired test --env=jsdom",
    "ftp": "node deploy",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "build-js": "react-app-rewired build",
    "analyze-boudle": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.2",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "express": "^4.14.0",
    "ftp-deploy": "^1.2.2",
    "html-loader": "^0.5.1",
    "install": "^0.10.2",
    "markdown-it-front-matter": "^0.1.2",
    "markdown-loader": "^2.0.1",
    "mockjs": "^1.0.1-beta3",
    "multer": "^1.2.1",
    "node-dir": "^0.1.17",
    "node-sass-chokidar": "^0.0.3",
    "nodemon": "^1.12.5",
    "npm": "^5.6.0",
    "npm-run-all": "^4.1.2",
    "react-app-rewire-define-plugin": "^1.0.0",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.3.7",
    "react-snapshot": "^1.3.0",
    "source-map-explorer": "^1.5.0"
  }
}
